{
  "jmake": {
    "prefix": "jmake",
    "body": [
      "MF=Makefile",
      "",
      "CC=g++",
      "CFLAGS=-O3",
      "LFLAGS=-lm",
      "",
      "EXE=main",
      "",
      "INC = \\",
      "\tlibmain.hpp \\",
      "\tmain_test.hpp",
      "",
      "SRC = \\",
      "\tmain.cpp \\",
      "\tlibmain.cpp \\",
      "\tmain_test.cpp",
      "",
      "############################################",
      "# No need to edit below this line",
      "#",
      "",
      ".SUFFIXES:",
      ".SUFFIXES: .cpp .o",
      "",
      "OBJ=$(SRC:.cpp=.o)",
      "",
      ".c.o:",
      "\t$(CC) $(CFLAGS) -c $<",
      "",
      "all:$(EXE)",
      "",
      "$(OBJ):$(INC)",
      "",
      "$(EXE):$(OBJ)",
      "\t$(CC) $(CFLAGS) -o $@ $(OBJ) $(LFLAGS)",
      "",
      "$(OBJ):$(MF)",
      "#############################################",
      "",
      "clean:",
      "\trm -f *.o $(EXE)"
    ],
    "description": "boilerplate make file"
  }
}

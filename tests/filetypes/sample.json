        "environment": {
          "DATABASE_URL": "${DATABASE_URL}"
        },
        "depends_on": [
          "db"
        ]
      },
      "db": {
        "image": "postgres:13",
        "restart": "always",
        "environment": {
          "POSTGRES_USER": "example_user",
          "POSTGRES_PASSWORD": "example_pass",
          "POSTGRES_DB": "example_db"
        },
        "volumes": [
